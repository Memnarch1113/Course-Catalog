<p id="notice"><%= notice %></p>

<h1>Listing Subjects</h1>

<table class=table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%=subject.name%></td>
        <td><%= link_to 'Show', subject %><%= link_to 'Edit', edit_subject_path(subject) %><%= link_to 'Destroy', subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subject', new_subject_path %>
